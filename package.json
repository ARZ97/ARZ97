    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "history": "4.10.1",
    "moment": "2.26.0",
    "mysql": "^2.18.1",
    "node-sass": "4.14.1",
    "nouislider": "14.5.0",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
